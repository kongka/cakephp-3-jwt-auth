{
    "name": "kongka/cakephp-3-jwt-auth",
    "type": "cakephp-plugin",
    "description": "CakePHP plugin for authenticating using JSON Web Tokens fork from admad/cakephp-jwt-auth",
    "keywords": [
        "cakephp",
        "authenticate",
        "authentication",
        "jwt"
    ],
    "homepage": "http://github.com/kongka/cakephp-3-jwt-auth",
    "authors": [
        {
            "name":"kongka",
            "role":"Author",
            "homepage":"https://github.com/kongka"
        }
    ],
    "license": "MIT",
    "support": {
        "source":"https://github.com/kongka/cakephp-3-jwt-auth",
        "issues":"https://github.com/kongka/cakephp-3-jwt-auth/issues"
    },
    "require": {
        "cakephp/cakephp": "^3.5",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "cakephp/chronos": "^1.1",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "kongka\\JwtAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kongka\\JwtAuth\\Test\\": "tests"
        }
    }
}
